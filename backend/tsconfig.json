{ 
  "compilerOptions":{ 
     "target":"es6",
     "module":"commonJS",
     "declaration": true,
     "emitDecoratorMetadata":true,
     "experimentalDecorators":true,
     "outDir":"dist/",
     "listFiles":true,
     "noEmitHelpers":false,
     "alwaysStrict": true,
     "suppressImplicitAnyIndexErrors": false,
     "forceConsistentCasingInFileNames": true,
     "noImplicitAny": false,
     "diagnostics": true,
     "esModuleInterop": true
  },
  "include":[ 
     "src/**/*"
  ],
  "exclude":[ 
     "node_modules",
  ],
  "compileOnSave":false
}